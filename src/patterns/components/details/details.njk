<details class="gv-c-details">
  <summary class="gv-c-details__summary">
    <span class="gv-c-details__summary-text">{{ summary }}</span>
  </summary>
  <div class="panel panel-border-narrow">
    {% if content %}
      {{ content }}
    {% endif %}
    {% if contentHtml %}
      {{ contentHtml | safe }}
    {% endif %}
  </div>
</details>
